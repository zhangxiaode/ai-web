{
  "name": "ai-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cdn": "gulp cdn",
    "clean": "rm -rf dist/*",
    "dev": "vite --mode development",
    "dev:test": "vite --mode test",
    "dev:stage": "vite --mode stage",
    "dev:pro": "vite --mode pro",
    "build:dev": "npm run clean && vue-tsc --noEmit && cross-env CDN=test vite build --mode development",
    "build:test": "npm run clean && vue-tsc --noEmit && cross-env CDN=test vite build --mode test",
    "build:stage": "npm run clean && vue-tsc --noEmit && cross-env CDN=test vite build --mode stage",
    "build": "npm run clean && vue-tsc --noEmit && node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --mode pro",
    "preview": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "echarts": "^5.6.0",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.5.4",
    "naive-ui": "^2.43.2",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.4",
    "unocss": "^66.5.10",
    "vue": "^3.2.25",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
