    "rollup-plugin-gzip": "^4.1.1",
    "sass": "^1.94.2",
    "terser": "^5.44.1",
    "through2": "^4.0.2",
    "typescript": "^5.9.3",
    "vfonts": "^0.0.3",
    "video.js": "^8.23.4",
